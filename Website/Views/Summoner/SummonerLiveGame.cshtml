@using System.Web;
@using Website.Helpers;
@model Website.Models.ViewModels.SummonerLiveGameViewModel

<div class="row">
    <div class="col-md-12">
        <div class="livegame-header">
            <div class="title"><span>@Model.LiveGame.GameMode</span><small>@Model.LiveGame.Map</small><small class="livetime">@Model.LiveGame.GameStartTime elapsed</small></div>
            <div class="bans">
                <div class="ban ban-blue">
                    @foreach (var ban in Model.LiveGame.Bans)
                    {
                        if (ban.Team == 100)
                        {<img src="@ban.ChampionUrl">}
                    }
                </div>
                <div class="ban ban-red">
                    @foreach (var ban in Model.LiveGame.Bans)
                    {
                        if (ban.Team == 200)
                        {<img src="@ban.ChampionUrl">}
                    }
                </div>
            </div>
        </div>
        <div class="">
            <table class="table table-condensed text-center" style="width:100%;margin-bottom:0px" cellspacing="0" cellpadding="0">
                <thead class="blue">
                    <tr>
                        <th></th>
                        <th>S6 Information</th>
                        <th>S6 Champion Information</th>
                        <th>S5</th>
                        <th>Rune / Mastery</th>
                    </tr>
                </thead>
                <tbody>
