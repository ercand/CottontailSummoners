                            <td>
                                <div class="champion-wrap">
                                    <div class="champion"> <a href="#"><img src="@champ.ChampionUrl"></a> </div>
                                    <div class="spells"> <img class="spell1" src="@champ.Spell1Url"> <img class="spell2" src="@champ.Spell2Url"> </div>
                                    <div class="name"><a href="/summoner/@Model.LiveGame.Region/@champ.Name">@champ.Name</a></div>
                                </div>
                            </td>
                            <td>
                                <div class="seasoninfo-wrap">
                                    @if (champ.Tier != 0)
                                    {
                                        @Html.Raw("<div class=\"division-wrap\"><img src=\"/images/tier_icons/" + Utility.TierToString(champ.Tier).ToLower() + "_" + Utility.RankedDivisionToString(champ.Division) + ".png\" \"> <span class=\"rank\">" + Utility.TierToString(champ.Tier) + " " + champ.Division + " (" + champ.LeaguePoint + ") </span></div>");
                                    }
                                    else
                                    {   @Html.Raw("<div class=\"division-wrap\"><img src=\"/images/tier_icons/provisional.png\" \"> <span class=\"rank\"> unranked </span></div>");
                                    }
                                    <div class="win-rate">
                                        <span class="ratio ">@(champ.RankedWinRate)%</span> ( <span class="TotalCount">@(champ.TotalRankedWin + champ.TotaleRankedLose) Played</span> )
                                        <div class="progress">
                                            <div class="fill" style="width: @(champ.RankedWinRate)%;"></div>
                                        </div>
                                    </div>
                                </div>
                            </td>
                            <td>
                                <div class="seasonChampionInfo-wrap">
                                    <div class="winRate-wrap"> <span class="ratio">@(champ.ChampionWinRate)%</span> ( <span class="gamePlayed">@(champ.TotalChampionRankedWin + champ.TotalChampionRankedLose) Played</span> ) </div>
                                    <div class="kda-wrap">
                                        <div class="kda"> <span class="value">@champ.ChampionKDA</span> KDA </div>
                                        <div class="kda-details">@champ.ChampionAverangeKill / @champ.ChampionAverangeDeath / @champ.ChampionAverangeAssist </div>
                                    </div>
                                </div>
                            </td>
                            <td>
